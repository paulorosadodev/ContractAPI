{
    "name": "contract-api",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "concurrently \"bun run server\" \"bun run client\"",
        "server": "bun run server/index.js",
        "client": "bunx --bun vite",
        "build": "bun run build:types && bunx --bun vite build",
        "build:types": "tsc -b",
        "start": "bun run server/index.js",
        "lint": "eslint .",
        "preview": "bunx --bun vite preview",
        "format": "prettier --write ."
    },
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "framer-motion": "^12.24.7",
        "lucide-react": "^0.562.0",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "ws": "^8.16.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@tailwindcss/vite": "^4.1.18",
        "@types/node": "^24.10.1",
        "@types/react": "^19.2.5",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react": "^5.1.1",
        "concurrently": "^8.2.2",
        "eslint": "^9.39.1",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.4.24",
        "globals": "^16.5.0",
        "prettier": "^3.6.2",
        "tailwindcss": "^4.1.18",
        "typescript": "~5.9.3",
        "typescript-eslint": "^8.46.4",
        "vite": "^7.2.4"
    }
}
